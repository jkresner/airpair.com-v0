          <option value="PST" selected>PST</option>
        </select>
        <input name="date" type="text" class="datepicker input-medium"
          value="{{../date}}" placeholder="pick a date">
        <select name="time">
          <option value="00:00" {{#is ../time '00:00'}}selected{{/is}}>00:00</option>
