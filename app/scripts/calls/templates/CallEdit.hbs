      <label class="control-label" for="duration">Duration</label>
      <div class="controls">
        <select name="duration">
          <option disabled value="{{../duration}}">
            {{../duration}} {{inflect ../duration "hour" "hours"}}
          </option>
        </select>
      </div>
    </div>
    <div class="control-group">
      <label class="control-label" for="date">Timezone / Date / Time
        (previously {{{callDateTime ../datetime}}})</label>
