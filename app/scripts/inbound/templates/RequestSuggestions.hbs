
<div class="expertMini suggestion">
  <div class="fullName">
    {{ name }}
    {{#if username }}<br />@{{ username }}
  {{/if}}
  </div>
  <hr />


  <img src="{{ pic }}?s=240" class="pic" />

  <div class="links">
    {{> DevLinks }}
  </div>

  <div class="tags">
    {{#each tags }}
      <span class="label label-tag">{{ short }}</span>
    {{/each }}
  </div>

  <a href="#" class="add" data-id="{{ _id }}">Add</a>
</div>



