<dl>
  <dt>Company</dt>
  <dd><a href="#" data-toggle="popover" data-content="{{about}}" data-trigger="hover">{{name}}</a></dd>
  {{#if url}}
  <dt style="display:none">Url</dt>
  <dd style="font-weight:normal"><a href="http://{{ url }}" target="_blank">http://{{ url }}</a></dd>
  {{/if}}
  <dt>Primary contact</dt>
  <dd class="contact">
    {{#each contacts}}

    <div>
      {{#if pic }}
        <img src="{{ pic }}" />
      {{else}}
        <img src="/images/avatars/default.jpg" />
      {{/if}}
    </div>

    <table>
      <tr>
        <th>Name</th>
        <td>{{fullName}}</td>
        <td style="width:20px"> </td>
        <td class="halfButtons"><a href="https://mixpanel.com/report/271245/explore/#list/filter:(conjunction:and,filters:!((filter:(operand:'{{fullName}}',operator:in),property:'$name',selected_property_type:string,type:string))),sort_order:descending,sort_property:'$created',sort_property_type:datetime" class="email btn btn-small btn-primary" target="_blank">MP</a><a href="https://mail.google.com/mail/u/0/?shva=1#search/%22{{fullName}}%22+OR+{{email}}" class="email btn btn-small btn-inverse" target="_blank"><i class="icon-envelope icon-white"></i> History</a>
        </td>
      </tr>
      <tr>
        <th>Email</th>
        <td>{{email}}</td>
        <td></td>
        <td>
          {{#if ../mailTmpls.incomplete}}
            <a id="receivedBtn" href="https://mail.google.com/mail/?view=cm&fs=1&tf=1&to={{email}}&su=MORE INFO REQUEST for your AirPair Session ({{../../threeTagsString}})&body=Hi {{firstName}},{{../../mailTmpls.incomplete}}" target="_blank" class="email btn btn-small btn-warning"><i class="icon-envelope icon-white"></i> Incomplete</a></td>
          {{/if}}

          {{#if ../mailTmpls.received}}
            <a id="receivedBtn" href="https://mail.google.com/mail/?view=cm&fs=1&tf=1&to={{email}}&su=Your Upcoming {{../../threeTagsString}} AirPair&body=Hi {{firstName}},{{../../mailTmpls.received}}" target="_blank" class="email btn btn-small btn-warning"><i class="icon-envelope icon-white"></i> Received</a></td>
          {{/if}}
      </tr>
      <tr>
        <th>Gmail</th>
        <td>{{gmail}}</td>
        <td></td>
        <td>
          {{#if ../mailTmpls.review}}
            <a href="https://mail.google.com/mail/?view=cm&fs=1&tf=1&to={{email}}&su=I've got you {{../../threeTagsString}} experts!&body=Hi {{firstName}},{{../../mailTmpls.review}}" target="_blank" class="email btn btn-small btn-warning"><i class="icon-envelope icon-white"></i> Review</a>
          {{/if}}
        </td>
      </tr>
      <tr>
        <th>Twitter</th>
        <td>{{#if twitter}}<a href="http://twitter.com/{{twitter}}" target="_blank">{{twitter}}</a>{{/if}}</td>
        <td></td>
        <td>
          {{#if ../mailTmpls.followup}}
            <a href="https://mail.google.com/mail/?view=cm&fs=1&tf=1&to={{email}}&su=How was your {{../../tagsString}} AirPair?&body={{firstName}},{{../../mailTmpls.followup}}" target="_blank" class="email btn btn-small btn-warning"><i class="icon-envelope icon-white"></i> How was?</a>
          {{/if}}
        </td>
      </tr>
      {{#if mixpanel}}
      {{#with mixpanel}}
      {{#if utm_campaign}}
      <tr>
        <th>Campaign</th>
        <td> {{utm_campaign}} </td>
      </tr>
      {{/if}}
      {{#if utm_content}}
      <tr>
        <th>Content</th>
        <td> {{utm_content}} </td>
      </tr>
      {{/if}}
      {{#if utm_medium}}
      <tr>
        <th>Medium</th>
        <td> {{utm_medium}} </td>
      </tr>
      {{/if}}
      {{#if utm_source}}
      <tr>
        <th>Source</th>
        <td> {{utm_source}} </td>
      </tr>
      {{/if}}
      {{#if utm_term}}
      <tr>
        <th>Term</th>
        <td> {{utm_term}} </td>
      </tr>
      {{/if}}
      {{/with}}
      {{/if}}
    </table>

    {{/each}}
  </dd>
</dl>

