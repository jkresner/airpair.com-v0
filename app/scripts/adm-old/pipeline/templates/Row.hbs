<td>
  <a class="edit{{createdAgoClass}}" href="#request/{{ _id }}" data-id="{{ _id }}">{{ status }}
      {{#if createdAgoClass}}<span class="label">{{ createdAgoString }}</span>{{/if}}
  </a>
      {{#unless createdAgoClass}}{{ createdDate }}{{/unless}}
</td>
<td>
  {{#if prevOwner}}
    <span class="label label-owner label-prev">prev: {{ prevOwner }}</span>
  {{else}}
    <span class="label label-owner label-{{ owner }}">{{ owner }}</span>
  {{/if}}
</td>
<td>
    {{#if contactPic }}
      <img src="{{ contactPic }}" />
    {{else}}
      <img src="/images/avatars/default.jpg" />
    {{/if}}
    <a href="mailto:{{contactEmail}}" target="_blank">{{ contactName }}</a>
    {{ company.name }}
</td>
<td>
  {{#each tags}}
    <span class="label label-tags">{{short}}</span>
  {{/each}}
</td>
<td>
  {{#each suggested }}
    {{#with expert}} <img class="border-{{../expertStatus}}" src="{{ pic }}" />{{/with}}
  {{/each}}
</td>
<td>
  {{#if callCount}}
    {{ callCompleteCount }} / {{ callCount }}
  {{/if}}
</td>
