
<h2>Edit Call</h2>
{{#expert}}
  <img src="{{pic}}" alt="{{name}}" draggable="false" class="expertPic">
  <br />
  {{name}}

<fieldset>
{{/expert}}

  {{#if errors}}
  <div class="alert-error">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <h4>Error</h4>
    {{#each errors}}
      Invalid {{param}}<br>
    {{/each}}
  </div>
  {{/if}}
</fieldset>
