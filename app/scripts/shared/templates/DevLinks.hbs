{{#if gh}}
<a href="//github.com/{{ gh.username }}" target="_blank" class="gh-white icon"></a>
{{/if}}


{{#if so}}
<a href="//stackoverflow.com/users/{{ so.link }}" target="_blank" class="so-white icon"></a>
{{/if }}

{{#if bb}}
<a href="//bitbucket.org/{{ bb.id }}" target="_blank" class="bb-white icon"></a>
{{/if}}

{{#if in}}
<a href="//linkedin.com/x/profile/sy5n2q8o2i49/{{ in.id }}" target="_blank" class="in-white icon"></a>
{{/if}}

{{#if tw}}
<a href="//twitter.com/{{ tw.username }}" target="_blank" class="tw-white icon"></a>
{{/if}}

{{#if sideproject}}
<a href="//{{ sideproject }}" target="_blank" class="sp-white icon">{{ sideproject }}</a>
{{/if}}

{{#if other}}
<a href="//{{ other }}" target="_blank" class="ot-white icon">{{ other }}</a>
{{/if}}

{{#if homepage}}
<a href="http://{{ homepage }}" target="_blank" class="hm-white icon"></a>
{{/if}}
