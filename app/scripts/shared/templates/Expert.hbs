<div class="expert">
  <h3>{{ name }}</h3>
  <h4>@{{ username }}</h4>
  <img src="{{ pic }}?s=240" class="pic" />

  <div class="links">
    {{> DevLinks }}
  </div>


  <div class="tags">
    {{#each tags }}
      <span class="label label-success">{{ short }}</span>
    {{/each }}
  </div>


</div>