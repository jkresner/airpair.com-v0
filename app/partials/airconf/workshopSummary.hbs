<div class="sessionCol">
  <a class="session user-badge text-center" href="/airconf/{{slug}}">
    <div class="user-info">
      <img src="//secure.gravatar.com/avatar/{{g}}?s=180" alt="{{n}}" class="img-circle" width="50%">
      <h3 class="name">{{n}}</h3>
      <h4>{{c}}</h4>
      <h5 class="tech">{{{t}}}</h5>
      <h6 class="title">{{#each tags}}<span>{ {{this}} }</span> {{/each}}</h6>
      <div class="description">
        <h6>{{{t}}}</h6>
        {{{a}}}
      </div>
    </div>
    <time datetime="{{{utc}}}"></time>
  </a>
  <div class="social-icons">


<a class="ap" href="/book/{{u}}?code=airconf"></a>
{{#if gh}}<a class="gh" target="_blank" href="//github.com/{{gh}}"></a>{{/if}}
{{#if so}}<a class="so" target="_blank" href="//stackoverflow.com/users/{{so}}"></a>{{/if}}
{{#if bb}}<a class="bb" target="_blank" href="{{bb}}"></a>{{/if}}
{{#if in}}<a class="in" target="_blank" href="//linkedin.com/x/profile/{{in}}"></a>{{/if}}
{{#if tw}}<a class="tw" target="_blank" href="//twitter.com/{{tw}}"></a>{{/if}}
</div>


</div>
