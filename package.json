{
  "author": "Jonathon Kresner",
  "name": "airpair.com",
  "description": "site hosted at airpair.com",
  "version": "0.5.1",
  "homepage": "http://www.airpair.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:jkresner/airpair.com.git"
  },
  "engines": {
    "node": "0.10.x",
    "npm": "1.2.x"
  },
  "scripts": {
    "test": "./build/git-hooks/pre-push",
    "postinstall": "brunch b -e prod",
    "hooks": "cd .git/hooks && ln -nsf ../../build/git-hooks/* .",
    "start": "PORT=3333 Env=dev runjs app.coffee",
    "start-test": "PORT=4444 Env=test runjs app.coffee",
    "coverage": "mocha --require blanket -R html-cov test/server/all.coffee > coverage.html && open coverage.html",
    "blanket": {
      "pattern": "/lib",
      "data-cover-never": "node_modules",
      "loader": "./node-loaders/coffee-script"
    }
  },
  "dependencies": {
    "coffee-script": "1.6.3",
    "javascript-brunch": "> 1.0 < 1.5",
    "coffee-script-brunch": "> 1.0 < 1.5",
    "css-brunch": "> 1.0 < 1.5",
    "handlebars-brunch": "> 1.0 < 1.5",
    "uglify-js-brunch": "> 1.0 < 1.5",
    "clean-css-brunch": "> 1.0 < 1.5",
    "brunch": "1.7.8",
    "underscore": "1.4.4",
    "async": "0.2.9",
    "express": "3.x",
    "express-validator": "~1.0.1",
    "mongoose": "3.8.5",
    "connect-mongo": "0.3.x",
    "passport": "0.1.x",
    "passport-oauth": "0.1.x",
    "passport-remember-me": "0.0.1",
    "passport-github": "0.1.x",
    "passport-google-oauth": "0.1.x",
    "passport-linkedin": "0.1.x",
    "passport-twitter": "0.1.x",
    "passport-facebook": "0.1.x",
    "passport-stackexchange": "0.1.x",
    "passport-bitbucket": "0.1.x",
    "moment": "2.0.x",
    "moment-timezone": "0.0.3",
    "superagent": "0.14.x",
    "winston": "~0.7.1",
    "winston-ses": "0.0.4",
    "handlebars": "1.x.x",
    "hbs": "2.3.x",
    "mixpanel": "0.0.18",
    "stripe": "1.3.0",
    "awssum-amazon-ses": "1.x.x",
    "googleapis": "~0.6.1",
    "lodash.clonedeep": "~2.4.1",
    "hipchatter": "0.1.x"
  },
  "devDependencies": {
    "mocha": "~1.12.0",
    "chai": "~1.7.2",
    "sinon": "1.4.2",
    "sinon-chai": "2.1.2",
    "supertest": "0.5.x",
    "blanket": "~1.1.5",
    "mocha-phantomjs": "~3.2.0",
    "run": "~1.2.0",
    "coffee-errors": "https://registry.npmjs.org/coffee-errors/-/coffee-errors-0.8.5.tgz"
  }
}
